{
  "buildCommand": "cd Src && npm install && npm run build",
  "outputDirectory": "Src/dist",
  "installCommand": "cd Src && npm install",
  "devCommand": "cd Src && npm run dev",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}